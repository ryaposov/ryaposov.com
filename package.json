{
  "name": "ryaposov",
  "version": "1.0.0",
  "description": "Blog and Portfolio",
  "author": "Pavel Ryaposov",
  "private": true,
  "scripts": {
    "dev": "nuxt --port=3002",
    "build": "cross-env ACTION=build nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "cross-env ACTION=build npm run build && gcloud app deploy staging.yaml --project ryaposov",
    "deploy-prod": "cross-env ACTION=build npm run build && gcloud app deploy production.yaml --project ryaposov"
  },
  "dependencies": {
    "@nuxt/http": "^0.5.0",
    "@nuxtjs/prismic": "^1.2.0",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/tailwindcss": "^1.4.0",
    "blotter.js": "git+https://github.com/bradley/Blotter.git",
    "dotenv": "^8.2.0",
    "mobile-detect": "^1.4.4",
    "nuxt": "^2.12.2",
    "nuxt-client-init-module": "^0.1.8",
    "prismic-vue": "^2.0.0",
    "vue-blotter": "^0.1.3",
    "web-streams-polyfill": "^2.1.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-mix-import-module-exports": "^0.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "nodemon": "^2.0.3",
    "nuxt-purgecss": "^1.0.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-nested-ancestors": "^2.0.0",
    "tailwindcss-theming": "^2.4.3"
  }
}
